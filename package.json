{
  "name": "restify-boilerplate",
  "version": "0.0.0-semantically-released",
  "description": "restify microservice boilerplate",
  "main": "src/serve.js",
  "repository": "git@github.com:shellthor/microservice-boilerplate.git",
  "author": "Jeff Detmer <jeff.detmer@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "^8.6.0",
    "npm": "^5.4.2",
    "yarn": "^1.1.0"
  },
  "scripts": {
    "add-contributor": "jmd-scripts contributors add",
    "build": "jmd-scripts build",
    "commit": "git-cz",
    "debug": "node --inspect --require babel-register src/serve | bunyan",
    "format": "jmd-scripts format",
    "lint": "jmd-scripts lint",
    "start": "nodemon src/serve --exec \"node --require babel-register\" | bunyan",
    "test": "jmd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "jmd-scripts validate"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/jmd-scripts/eslint.js"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "babel": {
    "presets": [
      "jmd-scripts/babel"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "bunyan": "1.8.12",
    "bunyan-sentry-stream": "1.2.1",
    "cross-env": "5.1.0",
    "dotenv": "4.0.0",
    "joi": "11.3.4",
    "jsonwebtoken": "8.1.0",
    "lodash": "4.17.4",
    "pg": "7.3.0",
    "pg-hstore": "2.3.2",
    "raven": "2.2.1",
    "restify": "6.2.0",
    "restify-cors-middleware": "1.0.1",
    "restify-errors": "5.0.0",
    "sequelize": "4.13.9"
  },
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "jmd-scripts": "1.1.1"
  }
}
